<%- include("partials/header") %>
<h1>Index page</h1>

<% blogs.forEach(function(blog){ %>
   
    <div>
        <h2><%=blog.title%></h2>
        <img src="<%=blog.image%>" >
        <span><%=blog.created%></span>
        <a href="/blogs/show/<%=blog._id%>">Read more</a>
    </div>

<%})%>

<%- include("partials/footer") %>
